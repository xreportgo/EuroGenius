# Variables d'environnement pour la production

# Configuration générale
NODE_ENV=production
PORT=3000

# Configuration de l'API
REACT_APP_API_URL=https://api.eurogenius.com/v1
REACT_APP_API_TIMEOUT=10000

# Configuration de l'authentification
REACT_APP_AUTH_DOMAIN=auth.eurogenius.com
REACT_APP_AUTH_CLIENT_ID=eurogenius-prod-client
REACT_APP_AUTH_REDIRECT_URI=https://www.eurogenius.com/callback

# Configuration des fonctionnalités
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_NOTIFICATIONS=true

# Configuration des services tiers
REACT_APP_PEDRO_MEALHA_API_KEY=your_production_api_key_here
